{
    "name": "vscode-xowl-languages",
    "displayName": "xOWL Languages Support",
    "description": "Language support for the RDF, OWL and xOWL languages",
    "icon": "xowl.png",
    "version": "0.0.1",
    "publisher": "cenotelie",
    "repository": {
        "type": "mercurial",
        "url": "https://bitbucket.org/xowl/xowl-vscode"
    },
    "license": "LGPL-3.0+",
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Languages"
    ],
    "keywords": [
        "language",
        "rdf",
        "owl",
        "xowl",
        "semantic"
    ],
    "activationEvents": [
        "onLanguage:xowlrdfnt"
    ],
    "files": [],
    "contributes": {
        "languages": [
            {
                "id": "xowlrdfnt",
                "aliases": [],
                "extensions": [
                    ".nt"
                ],
                "configuration": "./languages/rdf-nt-language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "xowlrdfnt",
                "scopeName": "source.xowl-rdf-nt",
                "path": "./languages/rdf-nt.tmLanguage.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "dependencies": {
        "vscode-languageclient": "3.3.0"
    },
    "devDependencies": {
        "typescript": "2.4.2",
        "vscode": "1.1.4",
        "@types/node": "^6.0.40",
        "@types/es6-promise": "^0.0.32"
    }
}