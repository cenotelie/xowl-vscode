{
    "name": "vscode-xowl-languages",
    "displayName": "xOWL Languages Support",
    "description": "Language support for the RDF, OWL and xOWL languages",
    "icon": "xowl.png",
    "version": "0.0.1",
    "publisher": "cenotelie",
    "repository": {
        "type": "mercurial",
        "url": "https://bitbucket.org/xowl/xowl-vscode"
    },
    "license": "LGPLv3",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Languages"
    ],
    "keywords": [
        "language",
        "rdf",
        "owl",
        "xowl",
        "semantic"
    ],
    "main": "./target/src/Main",
    "files": [],
    "contributes": {
        "languages": [
            {
                "id": "rdf",
                "aliases": [
                    "RDF"
                ],
                "extensions": [
                    ".nt",
                    ".nq",
                    ".ttl",
                    ".rdf",
                    ".jsonld",
                    ".trig",
                    ".xrdf",
                    ".ofn",
                    ".fs",
                    ".owx",
                    ".owl",
                    ".xowl"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "xowl",
                "aliases": [
                    "xOWL",
                    "xowl"
                ],
                "extensions": [
                    ".nt",
                    ".nq",
                    ".ttl",
                    ".rdf",
                    ".jsonld",
                    ".trig",
                    ".xrdf",
                    ".ofn",
                    ".fs",
                    ".owx",
                    ".owl",
                    ".xowl"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "xowl",
                "scopeName": "source.xowl",
                "path": "./syntaxes/xowl.tmLanguage.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "dependencies": {
        "portfinder": "^1.0.13",
        "vscode-languageclient": "3.2.2"
    },
    "devDependencies": {
        "typescript": "^2.2.1",
        "vscode": "^1.1.0",
        "@types/node": "^6.0.40",
        "@types/es6-promise": "^0.0.32"
    }
}